name = "Александр"
age = int(18)
name1 = input("Как вас зовут?")
if name == name1:
    print("У нас одинаковые имена")
    age1 = int(input("Сколько вам лет?"))
    if age == age1:
        print("А еще, у нас одинаковый возраст")
        exit()
    else:
        print("Приятно познакомиться,", name1)
        exit()
else:
    print("Приятно познакомиться,", name1)
age1 = int(input("Сколько вам лет?"))
if age < age1:
        print("Ты старше, чем я")
elif age == age1:
    print("Возраст у нас одинаковый")
else:
    print("Ты младше, чем я")
